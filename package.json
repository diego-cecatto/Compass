{
    "name": "compass-docgen",
    "version": "1.0.12-alpha",
    "author": {
        "name": "Diego Cecatto",
        "email": "diego.cecatto@gmail.com",
        "url": "https://github.com/diego-cecatto/Compass"
    },
    "bin": {
        "compass": "dist/commands/index.js"
    },
    "description": "This is a package that will help you to create your documentation for yours components",
    "main": "index.js",
    "scripts": {
        "pack": "tsc && npm pack",
        "start": "nodemon --watch dist --exec node dev",
        "type-check": "tsc --noEmit",
        "lint": "eslint src/**/*.ts src/**/*.tsx",
        "type:generate": "graphql-codegen",
        "server": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"./src/app/pages/component/live-editor/component.dependences.ts\" --exec \"ts-node index.ts\" -e ts",
        "updater": "ts-node updater.ts",
        "build": "node build.js"
    },
    "keywords": [
        "react",
        "documentation",
        "react-documentation",
        "documentation-generation"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/diego-cecatto/Compass.git",
        "directory": "build/compass"
    },
    "license": "ISC",
    "devDependencies": {
        "@apollo/client": "^3.7.17",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@graphql-codegen/cli": "^5.0.0",
        "@graphql-codegen/typescript-operations": "^4.0.1",
        "@graphql-codegen/typescript-react-apollo": "^3.3.7",
        "@mui/icons-material": "^5.14.3",
        "@mui/lab": "^5.0.0-alpha.138",
        "@mui/material": "^5.14.3",
        "@mui/x-data-grid": "^6.11.0",
        "@testing-library/react": "^14.0.0",
        "@types/babel__core": "^7.20.2",
        "@types/cli-progress": "^3.11.0",
        "@types/commander": "^2.12.2",
        "@types/express": "^4.17.17",
        "@types/fs-extra": "^11.0.1",
        "@types/jest": "^29.5.3",
        "@types/node": "^20.4.8",
        "@types/react": "^18.2.18",
        "@types/react-dom": "^18.2.7",
        "apollo-server": "^3.12.0",
        "apollo-server-express": "^3.12.0",
        "apollo-server-plugin-response-cache": "^3.8.2",
        "axios": "^1.4.0",
        "change-case": "^4.1.2",
        "cli-progress": "^3.12.0",
        "commander": "^11.0.0",
        "concurrently": "^8.2.0",
        "fs-extra": "^11.1.1",
        "graphql": "^16.7.1",
        "monaco-editor": "^0.41.0",
        "nodemon": "^3.0.1",
        "npm-check-updates": "^16.10.17",
        "path-browserify": "^1.0.1",
        "react": "^18.2.0",
        "react-docgen-typescript": "^2.2.2",
        "react-dom": "^18.2.0",
        "react-live": "^4.1.3",
        "react-markdown": "^8.0.7",
        "react-monaco-editor": "^0.54.0",
        "rehype-parse": "^8.0.4",
        "rehype-react": "^7.2.0",
        "sass": "^1.69.2",
        "ts-morph": "^19.0.0",
        "ts-node": "^10.9.1",
        "type-graphql": "^1.1.1",
        "typescript": "^5.1.6"
    },
    "browser": {
        "fs": false,
        "path": false,
        "os": false
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@apollo/utils.keyvadapter": "^3.0.0",
        "@babel/core": "^7.23.0",
        "@types/graphql": "^14.5.0",
        "@typescript-eslint/eslint-plugin": "^6.7.5",
        "@typescript-eslint/parser": "^6.7.5",
        "esbuild": "^0.19.4",
        "eslint": "^8.51.0",
        "eslint-config-react-app": "^7.0.1",
        "keyv": "^4.5.3",
        "serve-handler": "^6.1.5"
    }
}
