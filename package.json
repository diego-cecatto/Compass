{
    "name": "compass-docgen",
    "version": "1.0.29-alpha",
    "author": {
        "name": "Diego Cecatto",
        "email": "diego.cecatto@gmail.com",
        "url": "https://github.com/diego-cecatto/Compass"
    },
    "bin": {
        "compass": "dist/commands/index.js"
    },
    "description": "This is a package that will help you to create your documentation for yours components",
    "main": "index.js",
    "scripts": {
        "pack": "npm run build && npm pack",
        "type-check": "npm run build --noEmit",
        "dev": "nodemon --exec \"ts-node ./src/commands/index.ts dev\" -e ts",
        "prod": "node ./dist/commands/local/local.js",
        "updater": "ts-node ./src/commands/updater.ts",
        "lint": "eslint src/**/*.ts src/**/*.tsx",
        "type:generate": "graphql-codegen",
        "codegen": "graphql-codegen --config codegen.yml",
        "build": "tsc && copyfiles -f src/graphql/typedefs/* dist/graphql/typedefs"
    },
    "keywords": [
        "react",
        "documentation",
        "react-documentation",
        "documentation-generation"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/diego-cecatto/Compass.git",
        "directory": "build/compass"
    },
    "license": "ISC",
    "devDependencies": {
        "@apollo/utils.keyvadapter": "^3.1.0",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@graphql-codegen/cli": "5.0.0",
        "@graphql-codegen/client-preset": "4.1.0",
        "@graphql-codegen/introspection": "4.0.0",
        "@graphql-codegen/typescript": "^4.0.1",
        "@graphql-codegen/typescript-document-nodes": "4.0.1",
        "@graphql-codegen/typescript-operations": "^4.0.1",
        "@graphql-codegen/typescript-react-apollo": "^4.1.0",
        "@graphql-codegen/typescript-resolvers": "^4.0.1",
        "@mui/icons-material": "^5.14.15",
        "@mui/lab": "^5.0.0-alpha.150",
        "@mui/material": "^5.14.15",
        "@testing-library/react": "^14.0.0",
        "@types/cli-progress": "^3.11.4",
        "@types/compression": "1.7.5",
        "@types/cors": "^2.8.15",
        "@types/express": "^4.17.20",
        "@types/fs-extra": "^11.0.3",
        "@types/jest": "^29.5.6",
        "@types/node": "^20.8.9",
        "@types/react": "^18.2.33",
        "@types/react-dom": "^18.2.14",
        "@typescript-eslint/eslint-plugin": "^6.9.0",
        "@typescript-eslint/parser": "^6.9.0",
        "apollo-server-plugin-response-cache": "^3.8.2",
        "change-case": "^5.1.2",
        "cli-progress": "^3.12.0",
        "concurrently": "^8.2.2",
        "copyfiles": "^2.4.1",
        "eslint": "^8.52.0",
        "eslint-config-react-app": "^7.0.1",
        "graphql": "^16.8.1",
        "keyv": "^4.5.4",
        "nodemon": "^3.0.1",
        "path-browserify": "^1.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rehype-parse": "^9.0.0",
        "sass": "^1.69.5",
        "serve-handler": "^6.1.5",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    },
    "browser": {
        "fs": false,
        "path": false,
        "os": false
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "fs-extra": "^11.1.1",
        "@apollo/client": "^3.8.6",
        "@apollo/server": "^4.9.5",
        "@mui/x-data-grid": "^6.17.0",
        "@reduxjs/toolkit": "^1.9.7",
        "commander": "^11.1.0",
        "compression": "1.7.4",
        "cors": "^2.8.5",
        "esbuild": "^0.19.5",
        "graphql-http": "^1.22.0",
        "react-docgen": "^7.0.0",
        "react-live": "^4.1.5",
        "react-markdown": "^9.0.0",
        "react-redux": "^8.1.3",
        "react-router-dom": "^6.18.0"
    }
}
