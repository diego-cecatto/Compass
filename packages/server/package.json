{
    "name": "@compass-docgen/server",
    "version": "1.0.2",
    "author": {
        "name": "Diego Cecatto",
        "email": "diego.cecatto@gmail.com",
        "url": "https://github.com/diego-cecatto/Compass"
    },
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "bin": {
        "compass-server": "./cli/index.js"
    },
    "description": "This is a package to run your doc mate server",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon --exec \"ts-node ./src/commands/index.ts dev\" -e ts",
        "type:gen": "graphql-codegen --config codegen.yml",
        "build": "tsc && cpy graphql/typedefs/index.graphql ../../dist/@compass-docgen/server/graphql/typedefs/index.graphql"
    },
    "keywords": [
        "react",
        "documentation",
        "react-documentation",
        "documentation-generation"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/diego-cecatto/Compass.git",
        "directory": "build/compass"
    },
    "license": "MIT",
    "dependencies": {
        "@apollo/server": "^4.9.5",
        "@compass-docgen/core": "^1.0.2",
        "commander": "^11.1.0",
        "compression": "1.7.4",
        "cors": "^2.8.5",
        "cpy-cli": "^5.0.0",
        "graphql": "^16.8.1"
    },
    "devDependencies": {
        "@types/compression": "1.7.5",
        "@types/cors": "^2.8.15",
        "@types/express": "^4.17.20",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.1"
    },
    "browser": {
        "fs": false,
        "path": false,
        "os": false
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
