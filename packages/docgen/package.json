{
    "name": "compass-docgen",
    "version": "1.0.9",
    "author": {
        "name": "Diego Cecatto",
        "email": "diego.cecatto@gmail.com",
        "url": "https://github.com/diego-cecatto/Compass"
    },
    "bin": {
        "compass": "cli/index.js"
    },
    "description": "This is a package that will help you to create your documentation for yours components",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon --exec \"ts-node ./cli/index.ts dev\" -e ts",
        "prod": "node ./dist/commands/local/local.js",
        "build": "tsc && cpy compass ../../dist/compass-docgen/ && cpy compass.cmd ../../dist/compass-docgen/ && cpy compass.ps1 ../../dist/compass-docgen/"
    },
    "keywords": [
        "react",
        "documentation",
        "react-documentation",
        "documentation-generation"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/diego-cecatto/Compass.git",
        "directory": "build/compass-docgen"
    },
    "license": "ISC",
    "devDependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.14.15",
        "@mui/material": "^5.14.15",
        "@types/cli-progress": "^3.11.4",
        "@types/fs-extra": "^11.0.3",
        "@types/react": "^18.2.33",
        "@types/react-dom": "^18.2.14",
        "change-case": "^5.1.2",
        "nodemon": "^3.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.69.5",
        "ts-node": "^10.9.1"
    },
    "dependencies": {
        "@apollo/client": "^3.8.6",
        "@compass-docgen/core": "^1.0.2",
        "@compass-docgen/server": "^1.0.3",
        "@mui/x-data-grid": "^6.17.0",
        "@reduxjs/toolkit": "^1.9.7",
        "cli-progress": "^3.12.0",
        "commander": "^11.1.0",
        "esbuild": "^0.19.5",
        "fs-extra": "^11.1.1",
        "react-docgen": "^7.0.0",
        "react-live": "^4.1.5",
        "react-markdown": "^9.0.0",
        "react-redux": "^8.1.3",
        "react-router-dom": "^6.18.0"
    },
    "browser": {
        "fs": true,
        "path": true,
        "os": true
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
